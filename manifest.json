{
  "manifest_version": 3,
  "name": "Naps",
  "version": "1.0",


  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "action": {
    "default_title": "Naps highlighted",
    "default_popup": "kops.html",
    "default_icon": "k.png"
  },

  "permissions": ["activeTab", "scripting"]
}
